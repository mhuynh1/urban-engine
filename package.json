{
  "name": "ts-expressnodewebsocket",
  "version": "1.0.0",
  "description": "TypeScript + Express + Node + WebSocket",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"cyan.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "author": "mary huynh",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/ws": "^6.0.1",
    "typescript": "^3.1.2"
  }
}